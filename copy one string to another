#include <stdio.h>
#include <string.h>
int main() 
{
   char source_string[50], target_string[50];
   printf("Enter a string to copy: ");
   fgets(source_string, sizeof(source_string), stdin);
   strcpy(target_string, source_string);
   printf("Source string: %s", source_string);
   printf("Copied string: %s", target_string);
   return 0;
}
